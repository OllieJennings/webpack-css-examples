{
  "name": "webpack-css-examples",
  "version": "1.0.0",
  "description": "A template for CSS examples in Webpack",
  "main": "bootstrap.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:build": "./node_modules/.bin/webpack --config webpack/webpack.config.dev.js",
    "dev:server": "./node_modules/.bin/babel-node server.js",
    "prod:build": "./node_modules/.bin/webpack --config webpack/webpack.config.prod.js",
    "prod:server": "./node_modules/.bin/babel-node server.js",
    "start": "npm run dev:build && npm run dev:server",
    "prod": "npm run prod:build && set NODE_ENV=production&&npm run prod:server"
  },
